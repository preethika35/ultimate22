pipeline {
      agent any
      options {
            skipStagesAfterunstable()
      }
     stages  {
         stage('Rajkumar stage for demo') {
            steps {
              sh 'java --version'
              sh  'ls -l; lsblk
              sh  'echo  $BRANCH_NAME'
              }
           }

          stage('ramu stage' ) {
              steps {
                sh 'touch /tmp/ram.py'
                sh 'echo "helloworld" '
                sh  "python3  /tmp/ram.py"

              }
             }

      }
         





